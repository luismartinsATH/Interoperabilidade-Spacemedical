<?xml version="1.0" encoding="utf-8" ?>
<ExternalDatabaseSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  
  <!-- following setting indicates whether or not an external database is configured for use. -->
  <UseExternalDatabase>false</UseExternalDatabase>

  <!-- specifies the Web Service Url for external databases accessed via Web Wervices. -->
  <WebServiceUrl></WebServiceUrl>

  <!-- specifies the fully qualified type name of the external database, as known by Sentinel. -->
  <TypeName></TypeName>
   
</ExternalDatabaseSettings>
