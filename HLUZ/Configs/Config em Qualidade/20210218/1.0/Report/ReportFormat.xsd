    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="ReportTypeValue">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="RestingECG" />
        <xs:enumeration value="RhythmECG" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="PageTypeValue">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="NormalPage" />
        <xs:enumeration value="RhythmFrontPage" />
        <xs:enumeration value="RhythmContinuationPage" />
        <xs:enumeration value="Rhythm10SecStripPage" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="EcgScaleType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="TimesHalf" />
        <xs:enumeration value="TimesOne" />
        <xs:enumeration value="ReadFromSentinel" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="EcgSpeedType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="MmPerSec25" />
        <xs:enumeration value="ReadFromSentinel" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="PacingTraceType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="False" />
        <xs:enumeration value="ReadFromSentinel" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <!-- simple elements -->
    <xs:element name="Language">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="Name" type="xs:string" default="En-Us"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
  <xs:element name="Text">
      <xs:complexType>
        <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element ref="Language"/>
        </xs:sequence>
      </xs:complexType>
  </xs:element>
  <xs:element name="PageType">
    <xs:complexType>
      <xs:attribute ref="PageTypeValue" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ReportType">
    <xs:complexType>
      <xs:attribute ref="ReportTypeValue" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="PageOrientation" default="Landscape">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Landscape" />
        <xs:enumeration value="Portrait" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
    <xs:element name="Parameter">
    <xs:complexType>
      <xs:attribute name="Number" type="xs:unsignedByte" use="required" />
      <xs:attribute name="Id" type="xs:string" use="required" />
      <xs:attribute ref="Format" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Condition">
    <xs:complexType>
      <xs:attribute name="Param" type="xs:string" use="required" />
      <xs:attribute name="Value" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="SectionSize">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="SectionPos">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="StartNextSection">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Size">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Position">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
      <xs:attribute ref="OffsetType" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Alignment">
    <xs:complexType>
      <xs:attribute ref="Vertical" use="required" />
      <xs:attribute ref="Horizontal" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Font">
    <xs:complexType>
      <xs:attribute ref="FontType" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ShowECG">
    <xs:complexType>
      <xs:attribute name="Show" type="xs:boolean" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ShowBPMarkers">
    <xs:complexType>
      <xs:attribute name="Show" type="xs:boolean" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ShowGraticule">
    <xs:complexType>
      <xs:attribute name="Show" type="xs:boolean" use="required" />
      <xs:attribute ref="GraticuleSize" use="optional" default="Small"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ShowCallipers">
    <xs:complexType>
      <xs:attribute ref="CalliperType" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MoveTo">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
      <xs:attribute ref="OffsetType" use="optional" />
      <xs:attribute ref="Thickness" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DrawLineRel">
    <xs:complexType>
      <xs:attribute ref="X" use="required" />
      <xs:attribute ref="Y" use="required" />
      <xs:attribute ref="OffsetType" use="optional" />
      <xs:attribute ref="Thickness" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DrawLine">
    <xs:complexType>
      <xs:attribute ref="X0" use="required" />
      <xs:attribute ref="Y0" use="required" />
      <xs:attribute ref="X1" use="required" />
      <xs:attribute ref="Y1" use="required" />
      <xs:attribute ref="OffsetType" use="optional" />
      <xs:attribute ref="Thickness" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="LongComments">
    <xs:complexType>
      <xs:attribute name="Filename" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>
  <!--Label Str="I"/-->
  <xs:element name="TraceNumber">
    <xs:complexType>
      <xs:attribute name="Number" type="xs:int" use="optional" />
      <xs:attribute name="IsRhythmLead" type="xs:boolean" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="StartOffset">
    <xs:complexType>
      <xs:attribute name="MilliMetres" type="xs:float" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="SeqOffset">
    <xs:complexType>
      <xs:attribute name="MilliMetres" type="xs:float" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="SimOffset">
    <xs:complexType>
      <xs:attribute name="MilliMetres" type="xs:float" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Scale">
    <xs:complexType>
      <xs:attribute ref="EcgScaleType" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Speed">
    <xs:complexType>
      <xs:attribute ref="EcgSpeedType" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="PacingTrace">
    <xs:complexType>
      <xs:attribute ref="PacingTraceType" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="BitmapFile">
    <xs:complexType>
      <xs:attribute name="Filename" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Orientation">
    <xs:complexType>
      <xs:attribute name="Angle" type="xs:float" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ECGTrace">
    <xs:complexType>
      <xs:attribute name="Number" type="xs:int" use="required" />
      <xs:attribute ref="SCPCode" use="required" />
    </xs:complexType>
  </xs:element>
  <!-- composite elements -->
  <xs:element name="Definition">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Title" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="ReportType" />
        <xs:element ref="PageType" />
        <xs:element ref="PageOrientation"/>
        <xs:element name="CultureInfoName" type="xs:string" minOccurs="1" maxOccurs="1"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Page">
    <xs:complexType>
      <xs:attribute name="Filename" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DrawPolyLine">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Condition" maxOccurs="1" />
        <xs:element ref="MoveTo" maxOccurs="1" />
        <xs:element ref="DrawLineRel" minOccurs="1" maxOccurs="unbounded" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DrawText">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Condition" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Position" maxOccurs="1" />
        <xs:element ref="Size" maxOccurs="1" />
        <xs:element ref="Parameter" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="Alignment" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Font" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Text"  minOccurs="1" maxOccurs="unbounded"/>
        <xs:element ref="LongComments" minOccurs="0" maxOccurs="1" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ECGTraceMap">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="ECGTrace" maxOccurs="unbounded" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ECGStrip">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Condition" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Position" maxOccurs="1" />
        <xs:element ref="Size" maxOccurs="1" />
        <xs:element ref="TraceNumber" maxOccurs="1" />
        <xs:element ref="StartOffset" minOccurs="0" maxOccurs="1" />
        <xs:element ref="SeqOffset" minOccurs="0" maxOccurs="1" />
        <xs:element ref="SimOffset" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Scale" maxOccurs="1" />
        <xs:element ref="Speed" maxOccurs="1" />
        <xs:element ref="PacingTrace" maxOccurs="1" />
        <xs:element ref="ShowECG" maxOccurs="1" />
        <xs:element ref="ShowGraticule" maxOccurs="1" />
        <xs:element ref="ShowCallipers" maxOccurs="1" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MultiLineECG">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Condition" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Position" maxOccurs="1" />
        <xs:element ref="Size" maxOccurs="1" />
        <xs:element ref="TraceNumber" maxOccurs="1" />
        <xs:element ref="Scale" maxOccurs="1" />
        <xs:element ref="Speed" maxOccurs="1" />
        <xs:element ref="ShowGraticule" maxOccurs="1" />        
        <xs:element ref="ShowBPMarkers" maxOccurs="1" />
        <xs:element ref="PacingTrace" maxOccurs="1" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DrawBitmap">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Position" maxOccurs="1" />
        <xs:element ref="Size" maxOccurs="1" />
        <xs:element ref="BitmapFile" maxOccurs="1" />
        <xs:element ref="Orientation" maxOccurs="1" />
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="PageXOfY">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Position" maxOccurs="1" />
        <xs:element ref="Size" maxOccurs="1" />
        <xs:element ref="Parameter" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="Alignment" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Font" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Text"  minOccurs="1" maxOccurs="unbounded"/>
      </xs:choice>
      <xs:attribute name="Name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:attribute name="PageSize">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="A4" />
        <xs:enumeration value="Letter" />
       </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <!-- Top level elements -->
  <xs:element name="ReportFormat">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Definition" maxOccurs="1" />
        <xs:element ref="Page" maxOccurs="unbounded" />
        <xs:element ref="SectionPos" maxOccurs="1" />
        <xs:element ref="SectionSize" minOccurs="0" maxOccurs="1" />
        <xs:element ref="DrawPolyLine" maxOccurs="unbounded" />
        <xs:element ref="DrawLine" maxOccurs="unbounded" />
        <xs:element ref="DrawText" maxOccurs="unbounded" />
        <xs:element ref="ECGTraceMap" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ECGStrip" maxOccurs="unbounded" />
        <xs:element ref="MultiLineECG" maxOccurs="unbounded" />
        <xs:element ref="DrawBitmap" maxOccurs="unbounded" />
        <xs:element ref="StartNextSection" minOccurs="0" maxOccurs="1" />
        <xs:element ref="PageXOfY" minOccurs="0" maxOccurs="1" />
      </xs:choice>
      <xs:attribute ref="PageSize" use="optional"/>
    </xs:complexType>
  </xs:element>
</xs:schema>