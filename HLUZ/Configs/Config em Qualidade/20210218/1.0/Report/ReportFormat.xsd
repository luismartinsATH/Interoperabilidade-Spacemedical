<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ReportFormat" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!-- attributes -->
  <xs:attribute name="X" type="xs:float" />
  <xs:attribute name="Y" type="xs:float" />
  <xs:attribute name="X0" type="xs:float" />
  <xs:attribute name="Y0" type="xs:float" />
  <xs:attribute name="X1" type="xs:float" />
  <xs:attribute name="Y1" type="xs:float" />
  <xs:attribute name="OffsetType" default="Abs">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Abs" />
        <xs:enumeration value="DAbs" />
        <xs:enumeration value="Bottom" />
        <xs:enumeration value="NewLine" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name ="GraticuleSize" default="Small" >
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Small"/>
        <xs:enumeration value="Medium"/>
        <xs:enumeration value="Large"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="Thickness" default="Normal">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Normal" />
        <xs:enumeration value="Thick" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="Vertical" default="Below">
  <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Above" />
        <xs:enumeration value="Middle" />
        <xs:enumeration value="Below" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="Horizontal" default="Left">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Left" />
        <xs:enumeration value="Centre" />
        <xs:enumeration value="Right" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="FontType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="RomanNormalBold" />
        <xs:enumeration value="RomanSmall" />
        <xs:enumeration value="RomanNormal" />
        <xs:enumeration value="RomanLarge" />
        <xs:enumeration value="ArialLargeBold" />
        <xs:enumeration value="ArialNormalBold" />
        <xs:enumeration value="ArialSmall" />
        <xs:enumeration value="Arial8" />
        <xs:enumeration value="Arial8Ttf" />
        <xs:enumeration value="ArialNormal" />
        <xs:enumeration value="ArialLarge" />
        <xs:enumeration value="ArialSmallTtf" />
        <xs:enumeration value="ArialNormalTtf" />
        <xs:enumeration value="ArialNormalBoldTtf" />
        <xs:enumeration value="ArialLargeTtf" />
        <xs:enumeration value="ArialLargeBoldTtf" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="Format">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Text" />
        <xs:enumeration value="RTF" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="CalliperType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="RestingECG" />
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="SCPCode">
    <xs:simpleType>
